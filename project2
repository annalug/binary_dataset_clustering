    subgraph ENCODER["ğŸ”„ Encoder CNN (Compartilhado)"]
        E1["Conv2D 32 â†’ BatchNorm â†’ MaxPool"]
        E2["Conv2D 64 â†’ BatchNorm â†’ MaxPool"]
        E3["Conv2D 128 â†’ BatchNorm â†’ MaxPool"]
        E4["Flatten â†’ Dense 256"]
        E5["Dense 128 â†’ L2 Normalize"]
        E1 --> E2 --> E3 --> E4 --> E5
    end
    
    subgraph EMBEDDINGS["ğŸ¯ Embeddings"]
        EL["Embedding A<br/>(128,)"]
        ER["Embedding B<br/>(128,)"]
    end
    
    subgraph COMPARE["ğŸ“ ComparaÃ§Ã£o"]
        DIST["DistÃ¢ncia<br/>Euclidiana"]
        SIM["Sigmoid<br/>â†’ [0, 1]"]
        DIST --> SIM
    end
    
    subgraph OUTPUT["ğŸ“Š Resultado"]
        OUT["Similaridade<br/>0.85 = Muito similar<br/>0.15 = Muito diferente"]
    end
    
    DL --> ENCODER
    DR --> ENCODER
    ENCODER --> EL
    ENCODER --> ER
    EL --> DIST
    ER --> DIST
    SIM --> OUT
    
